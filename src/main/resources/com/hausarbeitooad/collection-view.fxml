<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="500.0" prefWidth="700.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.hausarbeitooad.controller.CollectionViewController">
   <children>
      <Separator layoutY="95.0" prefHeight="18.0" prefWidth="714.0" />
      <Label fx:id="gameNameID" alignment="CENTER" contentDisplay="TOP" layoutX="161.0" layoutY="51.0" prefHeight="18.0" prefWidth="403.0" text="Samplegame">
         <font>
            <Font size="36.0" />
         </font>
      </Label>
      <VBox layoutY="104.0" prefHeight="396.0" prefWidth="714.0">
         <children>
            <Label text="Spielzeit:">
               <font>
                  <Font size="36.0" />
               </font>
               <VBox.margin>
                  <Insets left="50.0" />
               </VBox.margin>
            </Label>
            <Label text="Erfolge:">
               <font>
                  <Font size="36.0" />
               </font>
               <VBox.margin>
                  <Insets left="50.0" />
               </VBox.margin>
            </Label>
            <Button fx:id="rezensionSchreibenID" mnemonicParsing="false" onAction="#handleOnActionRezensionSchreiben" text="Rezension schreiben">
