                  <Font size="36.0" />
               </font>
               <VBox.margin>
                  <Insets left="175.0" top="50.0" />
               </VBox.margin>
            </Button>
         </children>
      </VBox>
      <Button layoutX="14.0" layoutY="14.0" mnemonicParsing="false" onAction="#onActionCollectionBackBtn" prefHeight="18.0" prefWidth="6.0">
         <graphic>
            <ImageView fx:id="arrowLeftID1" fitHeight="32.0" fitWidth="32.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../../images/left-arrow.png" />
               </image>
            </ImageView>
         </graphic>
      </Button>
   </children>
</AnchorPane>
